{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-api-to-datalake"
		},
		"datalaketgtconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalaketgtconnection'"
		},
		"datalaketgtconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakekktgt.dfs.core.windows.net"
		},
		"restconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalaketgtconnection_accountKey": {
						"value": "[parameters('datalaketgtconnection_accountKey')]"
					},
					"datalaketgtconnection_properties_typeProperties_url": {
						"value": "[parameters('datalaketgtconnection_properties_typeProperties_url')]"
					},
					"restconnection_properties_typeProperties_url": {
						"value": "[parameters('restconnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}